{
  "name": "wix-rich-content-viewer",
  "version": "6.6.6",
  "author": {
    "name": "Jonathan Bachman",
    "email": "jonathanb@wix.com"
  },
  "main": "dist/module.cjs.js",
  "module": "dist/module.js",
  "style": "dist/styles.min.scss",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist/",
    "prebuild": "npm run clean",
    "build": "rollup --environment MODULE_NAME:viewer -c ../../../rollup.config.js",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:analyze": "npm run build -- --environment MODULE_ANALYZE && open stats.html",
    "watch": "cross-env NODE_ENV=development MODULE_WATCH=1 npm run build -- --watch.chokidar",
    "lint": "eslint 'src/**/*.{js,jsx}'"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "direction": "~1.0.3",
    "redraft": "~0.9.1",
    "wix-rich-content-common": "6.6.6"
  },
  "peerDependencies": {
    "@babel/runtime": "7.2.0",
    "lodash": "^4.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/runtime": "7.2.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "jest": "^23.1.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-test-renderer": "16.6.3",
    "rollup": "1.20.3"
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
